{
    "name": "tsanchev/yii2-asset-lightbox2",
    "type": "yii2-extension",
    "description": "Yii2 asset for bower LightBox2",
    "keywords": [
        "lightbox2",
        "asset",
        "extension",
        "yii2"
    ],
    "license": "BSD-3-Clause",
    "support": {
        "email": "tsanchev@gmail.com",
        "source": "https://github.com/tsanchev/yii2-asset-lightbox2"
    },
    "authors": [
        {
            "name": "Anton Tsanchev",
            "email": "tsanchev@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "bower-asset/lightbox2": "*"
    },
    "autoload": {
        "psr-4": {
            "tsanchev\\lightbox2\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
